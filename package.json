{
  "name": "jty",
  "version": "3.2.0",
  "description": "A minimal type checking library that I need in various projects for safer programming",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "docs": "typedoc",
    "build": "tsc",
    "test": "node --test",
    "test:watch": "node --test --watch",
    "preversion": "npm t",
    "prepublishOnly": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexewerlof/jty.git"
  },
  "keywords": [
    "javascript",
    "language",
    "manual type check",
    "type safety",
    "defensive programming",
    "security",
    "verification"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "typedoc": "^0.28.14",
    "typescript": "^4.2.3"
  }
}
