{
  "name": "jty",
  "version": "3.0.1",
  "description": "A minimal type checking library that I need in various projects for safer programming",
  "type": "commonjs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "docs": "typedoc src/index.ts --stripInternal",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "preversion": "npm t",
    "prepublishOnly": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/userpixel/jty.git"
  },
  "keywords": [
    "javascript",
    "language",
    "manual type check",
    "type safety",
    "defensive programming",
    "security",
    "verification"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "jest": "^26.6.3",
    "ts-jest": "^26.5.1",
    "typedoc": "^0.20.25",
    "typescript": "^4.1.5"
  }
}
